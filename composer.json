{
    "name": "esign/laravel-nova-testing",
    "description": "Testing toolkit for Laravel Nova.",
    "keywords": [
        "esign",
        "nova-testing"
    ],
    "homepage": "https://github.com/esign/laravel-nova-testing",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jordy Van der Haegen",
            "email": "jordy.vanderhaegen@dynamate.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.1|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Esign\\NovaTesting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esign\\NovaTesting\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esign\\NovaTesting\\NovaTestingServiceProvider"
            ],
            "aliases": {
                "NovaTesting": "Esign\\NovaTesting\\Facades\\NovaTestingFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
